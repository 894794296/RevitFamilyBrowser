<UserControl x:Class="RevitFamilyBrowser.WPF_Classes.GridSetup"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RevitFamilyBrowser.WPF_Classes"
             mc:Ignorable="d" 
             d:DesignHeight="870" Width="1200">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="155" Margin="25,30,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="240"/>

        <Canvas x:Name="canvas" HorizontalAlignment="Left" Height="800" Margin="270,30,0,0" VerticalAlignment="Top" Width="800" Background="Aquamarine"/>

    </Grid>
</UserControl>
